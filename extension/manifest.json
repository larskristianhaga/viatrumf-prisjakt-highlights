{
  "manifest_version": 3,
  "name": "Prisjakt Highlights",
  "version": "1.0",
  "description": "Highlights products on Prisjakt.no that are eligible for ViaTrumf cashback, or SAS Online Shopping points.",
  "icons": {
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://prisjakt.no/*",
        "https://*.prisjakt.no/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
